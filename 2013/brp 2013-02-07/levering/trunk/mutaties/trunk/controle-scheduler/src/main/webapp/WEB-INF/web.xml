<?xml version="1.0" encoding="UTF-8"?>
<web-app id="BRP-Controle-scheduler" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <display-name>BRP Controle Scheduler</display-name>

    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>

    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>classpath:/brp-admhnd-controle-context.xml</param-value>
    </context-param>

    <session-config>
        <session-timeout>60</session-timeout>
    </session-config>

</web-app>
