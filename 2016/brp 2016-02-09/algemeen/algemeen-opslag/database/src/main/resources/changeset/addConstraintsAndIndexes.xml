<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
    http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
">
    <preConditions onFail="HALT">
        <dbms type="postgresql"/>
    </preConditions>

    <changeSet id="addConstraintsAndIndexes" author="brp">
        <comment>Zet constraints en indexen terug op database.</comment>

        <sqlFile path="kern/helper/01-constraintsAdd.sql" splitStatements="false"/>
        <sqlFile path="kern/helper/01-indexesAdd.sql" splitStatements="false"/>
        <sqlFile path="kern/helper/92-sequencesUpdate.sql" splitStatements="false"/>
    </changeSet>
</databaseChangeLog>
